{
    "index_patterns": ["stocks"],
    "priority": 500,
    "composed_of": ["kaggle_component_template", "stocks-datastreams-settings", "datastream-mappings-default"],
    "version": 1,
    "_meta": {
        "description": "Template for stocks datastream."
    },
    "template": {
        "mappings": {
            "properties": {
                "symbol": {
                    "type": "keyword",
                    "fields": {
                        "wildcard": {
                            "type": "wildcard"
                        }
                    }
                },
                "date": {
                    "type": "date",
                    "ignore_malformed": true
                },
                "volume": {
                    "type": "scaled_float",
                    "scaling_factor": 100,
                    "ignore_malformed": true,
                    "fields": {
                        "raw": {
                            "type": "double",
                            "ignore_malformed": true
                        }

                    }
                },
                "open": {
                    "type": "scaled_float",
                    "scaling_factor": 100,
                    "ignore_malformed": true,
                    "fields": {
                        "raw": {
                            "type": "double",
                            "ignore_malformed": true
                        }

                    }
                },
                "close": {
                    "type": "scaled_float",
                    "scaling_factor": 100,
                    "ignore_malformed": true,
                    "fields": {
                        "raw": {
                            "type": "double",
                            "ignore_malformed": true
                        }

                    }
                },
                "high": {
                    "type": "scaled_float",
                    "scaling_factor": 100,
                    "ignore_malformed": true,
                    "fields": {
                        "raw": {
                            "type": "double",
                            "ignore_malformed": true
                        }

                    }
                },
                "low": {
                    "type": "scaled_float",
                    "scaling_factor": 100,
                    "ignore_malformed": true,
                    "fields": {
                        "raw": {
                            "type": "double",
                            "ignore_malformed": true
                        }
                    }
                }
            }
        }
    }
}
